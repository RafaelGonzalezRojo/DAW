public class C02Ejercicio02 {
	public static void main(String[] args) {
		String morado = "\033[35m";
		String blanco = "\033[37m";
		System.out.print(morado + "Direccion: Avd Ortega y gasset " + "\n Telefono: 687302814 \n" + blanco);

	}
}
